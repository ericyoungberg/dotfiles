!-## URxvt 

! Settings
URxvt.loginShell: true
URxvt.cursorBlink: true
URxvt.perl-ext-common: selection-to-clipboard
URxvt.clipboard.autocopy: false

! Font
URxvt.font: xft:Hack:pixelsize=11:antialias=true

! Window
URxvt.depth: 32
URxvt.scrollBar: false

! Color scheme
#define c_white        #FFFFFF
#define c_deep_purple  #72379b
#define c_east_purple  #9600FF
#define c_east_blue    #00B8FF
#define c_hot_pink     #FF00C1
#define c_neon_purple  #E810E8
#define c_neon_teal    #00FFF9
#define c_orange       #f29215
#define c_rich_orange  #F54F1E
#define c_rich_purple  #240036
#define c_soft_green   #2EF460
#define c_in_purple    #4A07A6
#define c_in_brown     #A66C1F
#define c_metal_red    #F54F1E
#define c_forest_green #1a541a

#define t_background   [90]#201220
#define t_foreground   c_orange

URxvt.background: t_background
URxvt.foreground: t_foreground

URxvt.color1: c_rich_orange
URxvt.color2: c_soft_green
URxvt.color3: c_orange
URxvt.color4: c_rich_orange
URxvt.color5: c_hot_pink
URxvt.color6: c_in_brown
URxvt.color7: c_soft_green
URxvt.color8: c_forest_green
URxvt.color9: c_soft_green
URxvt.color10: c_orange
URxvt.color11: c_in_brown
URxvt.color12: c_in_brown
URxvt.color13: c_in_brown
URxvt.color14: c_rich_orange
URxvt.color15: c_rich_orange
URxvt.color16: c_rich_orange

! vim: set ft=xdefaults:
