!-## URxvt 

! Settings
URxvt.loginShell: true
URxvt.cursorBlink: true
URxvt.perl-ext-common: selection-to-clipboard
URxvt.clipboard.autocopy: false

! Font
URxvt.font: xft:Hack:pixelsize=11:antialias=true

! Window
URxvt.depth: 32
URxvt.scrollBar: false

! Color scheme
#define t_background   [65]#2C101D
#define t_foreground   #FF3D23
#define c_candy_red    #FF0A0A
#define c_desert_sand  #DBD3AD
#define c_mute_pink    #D3AB9E
#define c_int_orange   #BF4E30
#define c_sizzle_red   #FF3A54

URxvt.background: t_background
URxvt.foreground: t_foreground

URxvt.color8: c_candy_red
URxvt.color9: c_desert_sand
URxvt.color10: c_mute_pink
URxvt.color14: c_int_orange
URxvt.color12: c_sizzle_red

! vim: set ft=xdefaults:
